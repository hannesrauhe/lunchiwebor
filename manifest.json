{
  "name": "Lunchiwebor",
  "description": "Lunchinator for Chrome",
  "manifest_version" : 2,
  "minimum_chrome_version": "33",
  "version" : "0.1",
  "app" : {
    "background" : {
	  "scripts" : ["app.js"]
    }
  },
  "permissions" : [
     "notifications"
  ],
  "sockets" : { 
	"udp": { 
        "send" : ["*:*"],
        "receive" : ["*:*"], 
		"bind" : ["*:*"]
	} 
  },
  "icons": {
  	"16":"images/lunch16.png",
	"48":"images/lunch48.png",
	"128":"images/lunch128.png"
	}
}
