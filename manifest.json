{
  "name": "Lunchiwebor",
  "description": "Lunchinator for Chrome",
  "manifest_version" : 2,
  "minimum_chrome_version": "33",
  "version" : "0.1",
  "app" : {
    "background" : {
	  "scripts" : ["app.js"]
    }
  },
  "permissions" : [
     "notifications"
  ],
  "sockets" : { 
	"udp": { 
		"send" : ["*:*"], 
		"bind" : ["*:*"]
	} 
  }
}
